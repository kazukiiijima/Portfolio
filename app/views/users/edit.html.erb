<html>
	<body>
		<div id="wrapper">
			<div class="post_create">
				<%= form_with model: @user, url: user_path, local: true do |f| %>
					<h2 class="post_new_item">
						画像
					</h2>
						<%= f.attachment_field :image %><br>
					<h2 class="post_new_item">
						ユーザー名
					</h2>
						<%= f.text_field :name %><br>
					<%= f.submit '変更を保存' %>
				<% end %>
			</div>
			<footer>
		      ©︎2021 Kazuki Iijima Portoforio
		    </footer>
		</div>
	</body>
</html>

